    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "openapi-mocks": "file:../../packages/openapi-mocks"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "typescript": "^5.9.3"
  }
}
